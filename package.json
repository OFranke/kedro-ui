{
  "name": "@quantumblack/carbon-ui-components",
  "version": "2.1.1",
  "description": "Carbon UI Components",
  "main": "dist/index.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run docs:server",
    "docs:server": "npm run fix:styleguidist && styleguidist server",
    "static": "node server.js",
    "build": "webpack --mode production",
    "build:dev": "webpack -d --config webpack.dev.config.js",
    "docs:build": "styleguidist build",
    "prepush": "npm run test",
    "prepublishOnly": "npm run test && npm run build",
    "stylelint": "stylelint templates/**/*.css --config stylelint.config.js",
    "eslint": "eslint src/**/*.js src/**/*.jsx src/**/*.test.js",
    "ava": "CONFIG=$(pwd)/webpack.config.js ava",
    "test:coverage": "nyc npm test",
    "test": "webpack && npm run ava",
    "test:watch": "npm run eslint && npm run ava -- --watch",
    "e2e": "npm run docs:build && cypress run --config fileServerFolder=styleguide,baseUrl=",
    "fix:styleguidist": "sed -i -e \"s/, ''//g\" node_modules/react-styleguidist/scripts/make-webpack-config.js"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/preset-react"
        ]
      }
    },
    "require": [
      "@babel/register",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "cobertura",
      "text",
      "html"
    ],
    "extension": [
      ".jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:quantumblack/asset-carbon-ui-components.git"
  },
  "author": "Quantumblack",
  "contributors": [
    {
      "name": "Andy Shora",
      "email": "andy.shora@quantumblack.com"
    },
    {
      "name": "Achilleas Tsoumitas",
      "email": "achilleas.tsoumitas@quantumblack.com"
    },
    {
      "name": "Susana Costa",
      "email": "susana.costa@quantumblack.com"
    },
    {
      "name": "Lucia Kocincova",
      "email": "lucia.kocincova@quantumblack.com"
    },
    {
      "name": "Ottis Kelleghan",
      "email": "ottis.kelleghan@quantumblack.com"
    },
    {
      "name": "Richard Westenra",
      "email": "richard.westenra@quantumblack.com"
    }
  ],
  "dependencies": {
    "@quantumblack/carbon-ui-core": "^1.1.1",
    "@quantumblack/javascript-standards": "^1.7.0",
    "autoprefixer": "^6.7.2",
    "browser-env": "^2.0.21",
    "chroma-js": "^1.2.2",
    "classnames": "^2.2.5",
    "es6-object-assign": "^1.0.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-require-jsdoc": "^1.0.4",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.10.1",
    "glob": "^7.1.1",
    "gsap": "^1.19.1",
    "ignore-styles": "^5.0.1",
    "lodash": "^4.17.11",
    "postcss-import": "^12.0.1",
    "postcss-scss": "^2.0.0",
    "prop-types": "^15.5.10",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-gsap-enhancer": "^0.3.0",
