{
  "name": "carbon-ui-components",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "ava": "CONFIG=$(pwd)/webpack.ava.config.js NODE_ENV=AVA BABEL_DISABLE_CACHE=1 ava",
    "build": "npm test && webpack -d",
    "docs:server": "styleguidist server",
    "docs:build": "styleguidist build",
    "eslint": "eslint source/*.js source/**/*.js source/**/*.jsx",
    "precommit": "npm test",
    "prepublish": "npm run test",
    "start": "npm run icons && npm run docs:server",
    "test": "npm run eslint && npm run icons && npm run ava"
  },
  "ava": {
    "files": [
      "source/**/*.test.js"
    ],
    "verbose": true,
    "babel": "inherit",
    "require": [
      "babel-register",
      "ignore-styles"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/quantumblack/carbon-ui-components.git"
