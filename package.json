{
  "name": "@quantumblack/carbon-ui-components",
  "version": "0.6.4",
  "description": "Carbon UI Components",
  "main": "dist/index.min.js",
  "scripts": {
    "start": "npm run docs:server",
    "docs:server": "styleguidist server",
    "static": "node server.js",
    "build": "webpack -p --config webpack.build.config.js",
    "build:dev": "webpack -d --config webpack.dev.config.js",
    "docs:build": "styleguidist build",
    "postinstall": "npm run build:dev && npm run build",
    "prepush": "npm run test",
    "prepublish": "npm run test",
    "stylelint": "stylelint templates/**/*.css --config stylelint.config.js",
    "eslint": "eslint src/**/*.js src/**/*.jsx",
    "ava": "CONFIG=$(pwd)/webpack.ava.config.js NODE_ENV=AVA BABEL_DISABLE_CACHE=1 ava",
    "test:coverage": "nyc npm test",
    "test": "npm run eslint && npm run ava",
    "test:watch": "npm run eslint && npm run ava -- --watch",
    "e2e": "npm run docs:build && cypress run --config fileServerFolder=styleguide,baseUrl="
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "src/**/*.test.js"
    ],
    "verbose": true,
    "babel": "inherit",
    "require": [
      "babel-register",
      "ignore-styles",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "cobertura",
      "text",
      "html"
    ],
    "extension": [
      ".jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/quantumblack/carbon-ui-components.git"
  },
  "author": "Andy Shora <andy.shora@quantumblack.com>",
  "contributors": [
    {
      "name": "Andy Shora",
      "email": "andy.shora@quantumblack.com"
    },
    {
      "name": "Achilleas Tsoumitas",
      "email": "achilleas.tsoumitas@quantumblack.com"
    },
    {
      "name": "Susana Costa",
      "email": "susana.costa@quantumblack.com"
    },
    {
      "name": "Lucia Kocincova",
      "email": "lucia.kocincova@quantumblack.com"
    },
    {
      "name": "Ottis Kelleghan",
      "email": "ottis.kelleghan@quantumblack.com"
    },
    {
      "name": "Richard Westenra",
      "email": "richard.westenra@quantumblack.com"
    }
  ],
  "dependencies": {
    "@quantumblack/carbon-ui-core": "^1.1.1",
    "@quantumblack/javascript-standards": "^1.7.0",
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.18.0",
    "chroma-js": "^1.2.2",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "es6-object-assign": "^1.0.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.1",
    "glob": "^7.1.1",
    "gsap": "^1.19.1",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.2",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.3.0",
    "postcss-map": "^0.8.0",
    "precss": "^1.4.0",
    "prop-types": "^15.5.0",
    "react": "^15.5.4",
    "react-dom": "^15.4.2",
    "react-gsap-enhancer": "^0.3.0",
    "react-styleguidist": "5.0.0-beta.15",
    "style-loader": "^0.13.1",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-order": "^0.3.0",
    "svg-react-loader": "^0.3.7",
    "svgo-loader": "^1.1.2",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "browser-env": "^2.0.21",
    "enzyme": "^2.8.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-require-jsdoc": "^1.0.4",
    "ignore-styles": "^5.0.1",
    "nyc": "^10.1.2",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "sinon": "^1.17.7"
  },
  "engines": {
    "node": "6.9",
    "npm": "3.10"
  },
  "license": "UNLICENSED",
  "homepage": "https://bitbucket.org/quantumblack/carbon-ui-components#readme"
}
